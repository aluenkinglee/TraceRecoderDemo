<![CDATA[
        <Placemark>\n
        	<visibility>1</visibility>\n
        	<LineString>\n
        		<coordinates>\n
        			%1$s\n
        		</coordinates>\n
        	</LineString>\n
        </Placemark>\n
        ]]>
