    <string name="singlePointXML"><Data>
<![CDATA[
        <Placemark>\n
        	<visibility>1</visibility>\n
        	<Point>\n
        		<coordinates>\n
        			%1$s\n
        		</coordinates>\n
        	</Point>\n
        	<description>\n
        	<![CDATA[\n
        		<script>\n
        			data = %2$s\n
        		</script>\n
        		<body style=\"width: 65536px; height: 65536px;\">\n	
        			<iframe name=\"jsCall\" width=\"0\" height=\"0\" src=\"gallery.html\"></iframe>\n
        			<div id=\"wrapper\" style=\"width: 100%%; height: 100%%;\"></div>\n
        		</body>\n
        	]]]]><![CDATA[>\n
        	</description>\n
        </Placemark>\n
        ]]>
</Data></string>
